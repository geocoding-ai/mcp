{
  "name": "@geocoding-ai/mcp",
  "type": "module",
  "version": "dev",
  "description": "Model Context Protocol server for geocoding",
  "keywords": [
    "mcp-server",
    "mcp",
    "geocoding",
    "reverse-geocoding",
    "geocoding-ai",
    "nominatim",
    "openstreetmap",
    "osm",
    "geojson",
    "geocodejson"
  ],
  "author": {
    "name": "Srihari Thalla",
    "email": "daxserver@icloud.com"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "tag": "latest"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
    "inspect": "bunx @modelcontextprotocol/inspector node dist/index.js",
    "lint": "eslint src",
    "prepublishOnly": "bun run build && chmod 755 dist/index.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/bun": "1.2.19",
    "eslint": "^9.30.0",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "typescript-eslint": "^8.35.0"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.12.1",
    "axios": "^1.10.0"
  }
}
